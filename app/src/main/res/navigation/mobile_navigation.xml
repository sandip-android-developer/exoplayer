<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.mediasupportingapplication.ui.image.ImageFragment"
        android:label="@string/title_image"
        tools:layout="@layout/fragment_image" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.mediasupportingapplication.ui.multimedia.MultiMediaFragment"
        android:label="@string/title_multimedia"
        tools:layout="@layout/fragment_multimedia" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.mediasupportingapplication.ui.file.fileFragment"
        android:label="@string/title_file"
        tools:layout="@layout/fragment_file" />
</navigation>